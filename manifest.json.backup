{
    "name": "Mothership on Main",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Sets the Mothership on Main page as your default home and new tab page",
    "chrome_url_overrides": {
        "newtab": "index.html",
        "homepage": "index.html"
    },
    "permissions": ["tabs", "storage"],
    "browser_action": {
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    }
}