{
    "name": "Mothership on Main",
    "version": "1.1",
    "manifest_version": 3,
  "description": "Sets the Mothership on Main page as your default home and new tab page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
    "action": {
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    }
}

